{
    "name": "@frankhu00/react-google-maps",
    "version": "1.4.0",
    "description": "Google maps for react",
    "main": "dist/umd/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "source": "src/index.ts",
    "files": [
        "dist/",
        "src/"
    ],
    "engines": {
        "node": ">=14.15.0"
    },
    "scripts": {
        "clean": "rimraf dist",
        "build": "npm run clean && npm run build:esm && npm run build:umd && npm run build:types",
        "build:types": "npx tsc --noEmit false --declaration --emitDeclarationOnly --outDir dist/types",
        "build:esm": "cross-env BABEL_ENV=production-esm babel src -d dist/esm --ignore \"**/*.spec.ts,**/*.spec.tsx\" --extensions \".ts,.tsx\" --copy-files",
        "build:umd": "cross-env BABEL_ENV=production-umd babel src -d dist/umd --ignore \"**/*.spec.ts,**/*.spec.tsx\" --extensions \".ts,.tsx\" --copy-files",
        "dev": "cross-env NODE_ENV=development webpack serve",
        "test": "jest"
    },
    "keywords": [
        "React",
        "Google Maps"
    ],
    "author": "Frank Hu",
    "license": "ISC",
    "peerDependencies": {
        "react": ">=17",
        "react-dom": "^17.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.10",
        "@babel/core": "^7.12.10",
        "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-react": "^7.12.10",
        "@babel/preset-typescript": "^7.14.5",
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@svgr/webpack": "^5.5.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.0.0",
        "@testing-library/react-hooks": "^7.0.1",
        "@testing-library/user-event": "^13.2.1",
        "@types/google.maps": "^3.45.6",
        "@types/jest": "^26.0.24",
        "@types/react": "^17.0.11",
        "@types/uuid": "^8.3.1",
        "babel-jest": "^27.0.6",
        "babel-loader": "^8.2.2",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "cross-env": "^7.0.3",
        "css-loader": "^6.2.0",
        "git-cz": "^4.7.1",
        "html-webpack-plugin": "^4.5.0",
        "jest": "^27.0.6",
        "prettier": "^2.2.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "rimraf": "^3.0.2",
        "style-loader": "^3.2.1",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.1.0",
        "typescript": "^4.3.2",
        "webpack": "^5.11.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/git-cz"
        }
    },
    "dependencies": {
        "antd": "^4.16.11",
        "use-deep-compare-effect": "^1.6.1",
        "uuid": "^8.3.2"
    }
}
